{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    },
    "logLevel": {
      "default": "Information",
      "Host": "Error",
      "Function": "Error"
    }
  },
  "extensions": {
    "durableTask": {
      "hubName": "TaskApprovalHub"
    }
  }
}